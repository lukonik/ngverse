{
  "name": "ng-verse",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lukonik/ng-verse"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=docs",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --project=ng-verse",
    "lint": "ng lint",
    "prepare": "husky",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "semantic-release": "semantic-release",
    "serve:ssr:docs": "node dist/docs/server/server.mjs",
    "schema:test-watch": "nodemon --watch dist/ng-verse --ext js --exec 'jasmine dist/ng-verse/**/*_spec.js'",
    "build:ng-verse": "ng build --project=ng-verse && cd projects/ng-verse && npm run build"
  },
  "lint-staged": {
    "**/*.{html,ts,json,css,scss,md}": "prettier --write"
  },
  "dependencies": {
    "@angular/animations": "^19.0.6",
    "@angular/cdk": "^19.0.5",
    "@angular/common": "^19.0.6",
    "@angular/compiler": "^19.0.6",
    "@angular/core": "^19.0.6",
    "@angular/forms": "^19.0.6",
    "@angular/platform-browser": "^19.0.6",
    "@angular/platform-browser-dynamic": "^19.0.6",
    "@angular/platform-server": "^19.0.6",
    "@angular/router": "^19.0.6",
    "@angular/ssr": "^19.0.7",
    "@types/file-saver": "^2.0.7",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "i": "^0.3.7",
    "jszip": "^3.10.1",
    "lucide-angular": "^0.462.0",
    "ngx-highlightjs": "^12.0.0",
    "npm": "^10.9.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.7",
    "@angular-devkit/schematics": "^19.0.7",
    "@angular/cli": "^19.0.7",
    "@angular/compiler-cli": "^19.0.6",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-angular": "^19.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "angular-eslint": "19.0.2",
    "autoprefixer": "^10.4.20",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "husky": "^9.1.7",
    "jasmine": "^5.5.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.3.0",
    "ng-packagr": "^19.0.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.49",
    "prettier": "3.4.2",
    "semantic-release": "^24.2.1",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.6.2",
    "typescript-eslint": "8.18.0"
  }
}
